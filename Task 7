#include <iostream>
using namespace std;

int main() {
    int n, count = 0;

    // Ask the user to input a number n
    cout << "Enter a number n: ";
    cin >> n;

    // Loop 
    for (int i = 1; i <= n; i++) {
        // Check if the number is divisible by 3
        if (i % 3 == 0) {
            count++;  
        }
    }

    // Output 
    cout << "There are " << count << " numbers divisible by 3 between 1 and " << n << "." << endl;

    return 0;
}
